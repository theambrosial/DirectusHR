  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts --max_old_space_size=8096 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "collect": "react-scripts build && npm run build-rename && npm run copy-buildfiles && npm run collectstatic",
    "build-rename": "npm run build-rename-js && npm run build-rename-chunk-js && npm run build-rename-normal-chunk-js && npm run build-rename-css && npm run build-rename-chunk-css",
    "build-rename-js": "renamer --regex --find \"runtime-main\\.[^\\.]+\\.js\" --replace \"react-django.ui.js\" build/static/js/*.js",
    "build-rename-chunk-js": "renamer --regex --find \"main\\.[^\\.]+\\.js\" --replace \"react-django.chunk.js\" build/static/js/*.js",
    "build-rename-normal-chunk-js": "renamer --regex --find \"\\.[^\\.]+\\.chunk.js\" --replace \"react-django.normal-chunk.js\" build/static/js/*.js",
    "build-rename-css": "renamer --regex --find \"main\\.[^\\.]+\\.css\" --replace \"react-django.ui.css\" build/static/css/*.css",
    "build-rename-chunk-css": "renamer --regex --find \"\\.[^\\.]+\\.chunk.css\" --replace \"react-django.chunk.css\" build/static/css/*.css",
    "copy-buildfiles": "npm run copy-build-js && npm run copy-build-css && npm run copy-build-logo",
    "copy-build-logo": "copyfiles -f \"build/static/media/*\" \"../static_local/img/\"",
    "copy-build-js": "copyfiles -f \"build/static/js/*.js\" \"../static_local/js/\"",
    "copy-build-css": "copyfiles -f \"build/static/css/*.css\" \"../static_local/css/\"",
    "collectstatic": "python ../manage.py collectstatic --no-input"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
